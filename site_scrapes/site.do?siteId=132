				,type: 10
				
				
					,variableType: 0
				
					
				
				
				
					
								
				
					
				
				
					,extensions: "bmp,jpg,gif,png,tif"
				
			});
			
			
			
			if (hmax < 280) {
				hmax = 280;
			}

		
		
		if (hmax != 0) {
			hmax += 20;
			if(hmax > 400) $("#div-input").css({height: hmax +"px"});
			else $("#div-input").css({height: "400px"});
		} else {
			$("#div-input").css({height: "400px"});
		}
		
		
			calculEnable = true;
		
	
		if(!isPreviewable){
			$("#button-preview-data").hide();
		}else{
			$("#button-preview-data").unbind("click");
			$("#button-preview-data").click(function(){
				if(!isPreviewable) return false;
				
				$("#div-preview-data").toggle('blind');
				 
				if(!isPreview){
					isPreview = true;
					previewData();
					$("#button-preview-data").html("Close preview data");
				}else{
					isPreview = false;
					$("#button-preview-data").html("Preview data");
				}

			});
		}
		
		
		
		
		
		$("#select-demos-datas").unbind("click");
		$("#select-demos-datas").click(function(){
			
			
			
				$('#input-1312').fieldcontrol('reset');
			
			
			isDemoData = false;
			
			
		    
		    	$("#button-run-my-code").removeClass("button-disable"); 
	    		$("#button-run-my-code").addClass("button-enable");
		    
		    
			$("#cs-tab-bottom").tabs('disable', 3);
			$("#div-message").hide();
			
			if(isPreview){
				$("#div-preview-data").toggle('blind');
				isPreview = false;
				$("#button-preview-data").html("Preview data");
			}
			
			var value = $("#select-demos-datas").val();
			
			if(value != 0){
				
