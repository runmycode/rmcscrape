<script type="text/javascript">
	var is_computing = false;
	
	function previewData() {
		
			
		
	}
	
	/**
	 * Lance le calcul et place le numero du site dans la liste des calculs en cours
	 */
	function compute() {
		var params = {};
		params["site"] = 132;

		
	
	    $.ajax({
	    	url : "task.do?action=add-task" ,
	    	type : "POST",
	    	async: true,
	    	dataType : "json",
	    	data : params,
	    	success: function(data){	
	    		// Serveur KO
	    		if(data == null){
	    			addMessage(errorMessage2, 1, "div-message");
	    		}else{
		    		// nombre calcul en cours KO
		    		if(data[0].isMaxComputesReached){
						addMessage(errorMessage6, 1, "div-message");
					// nombre calcul en cours PK
					}else{
						// DATA OK
						if(data[0].isDataOk){
							// Serveur OK
							if(data[0].isTaskAdd){
								displayEtatAvancement(2);
								
									addMessage("Computation in process. Please check the status of your computation in the computing queue/past results (left menu). You can leave the website and return later to retrieve your results.", 3, "div-message");
								
								
								setTimeout(function(){displayEtatAvancement(4);}, 6000);
								
							// Serveur KO
							}else{	
								addMessage(errorMessage1, 1, "div-message");
							}
						// DATA KO
						}else{
							addMessage(errorMessage2, 1, "div-message");
						}
					}
	    		}
	    		
	    		unWaitIt();
	    	},
	    	// Serveur KO
	    	error : function(data){
	    		addMessage(errorMessage1, 1, "div-message");
	    		unWaitIt();
	    	}
	    });
	}
	
	function waitIt(){
		$.ajax({
			async: true,
			success: function(){
				is_computing = true;
				$("#button-wait").html('<img src="images/icones/wait.gif" alt="loading" />');
			}
		});
	}
	
	function unWaitIt(){
		$.ajax({
			async: true,
			success: function(){
				is_computing = false;
				$("#button-wait").html('');
			}
		});
	}
	
	$(document).ready(function(){
		
		var isDemoDataExist = false;
		
		
		
		
			$.ajax({
		        url: '/data/MetaSite/upload/companionSite132/data/demo-result.pdf', 
		        method: 'post',
		        async: false,
		        error: function(XMLHttpRequest, textStatus, errorThrown){isDemoDataExist = false;},
		        success: function(data){isDemoDataExist = true;}
		    });
		
		
		// TODO
		isDemoDataExist = true;
		
		$("#button-run-my-code").unbind("click");
		$("#button-run-my-code").click(
			function(){
				confirmAddInSelection(EXECUTE_CODE);
			
				if(is_computing) return false;
				waitIt();
				
				/*$.ajax({
					async: true,
					type: "GET",
					success: function(){*/
				
						
						
						
							if (!isDemoData) {
								
								/**
								 * si pas de membre en session, on demande l identification
								 */
								
							
									
										unWaitIt();
										$('#button-login').click();
									
									
									// on execute si c'est une site anonyme
									
									
								
								
								/**
								 * sinon on prepare le calcul
								 */
								 
								
							} else {
								registerAction(EXECUTE_DEMO_DATA);
								if(!isDemoDataExist){
									$("#dialog:ui-dialog").dialog("destroy");
									$("#dialog").html('This companion website is temporarily down for maintenance. It should be back online shortly');
									$("#dialog").dialog({
										title: "Info",
										dialogClass: "dialog-error",
										resizable: false,
										modal: true,
										draggable: false,
										buttons: {
											"OK": function(){
												unWaitIt();
												$(this).dialog("close");
											}
										}
									});
								}else{
									unWaitIt();
									
									
										_gaq.push(['_trackPageview', '/companionSite132/data/demo-result.pdf']);
										displayEtatAvancement(3);
									
									
									
								}
							}
						
					/*}
				});*/
			}
		);
		
		$("#button-load-demo-data").unbind("click");
		$("#button-load-demo-data").click(
			function(){
				confirmAddInSelection(LOAD_DEMO_DATA);
				$("#div-message").hide();

				
					$('#input-1312').fieldcontrol({rows_current: 0, columns_current: 0});		
					$('#input-1312').fieldcontrol('getDemoData');
				
				
			    if(isDemoDataExist){
			    	$("#button-run-my-code").removeClass("button-disable");
					$("#button-run-my-code").addClass("button-enable");
			    }else{
			    	$("#button-run-my-code").removeClass("button-enable"); 
					$("#button-run-my-code").addClass("button-disable");
			    }
			    
			    isDemoData = true;
			}
		);
		
		$("#button-reset-data").unbind("click");
		$("#button-reset-data").click(
			function(){
				
				
					$('#input-1312').fieldcontrol('reset');
				
				
				isDemoData = false;
				
				
			    
			    	$("#button-run-my-code").removeClass("button-disable"); 
		    		$("#button-run-my-code").addClass("button-enable");
			    
			    
				$("#cs-tab-bottom").tabs('disable', 3);
				$("#div-message").hide();
				
				if(isPreview){
					$("#div-preview-data").toggle('blind');
					isPreview = false;
					$("#button-preview-data").html("Preview data");
				}
			
			}
		);

		/*
		 *calcul de la hauteur de div-input
		 */
		var hmax = 0;
		
		/*Gestion affichage des inputs*/
		

			/* Gestion des contraintes*/
			$('#input-1312').fieldcontrol({
				id: 1312
				,site: 132
				,name: $('#label-1312-hold').html()
				
				// Position
				,top: 80
				,left: 85
				
