				}else if(value == 6){
					$("#input-1450").val("ln_a~=. ");
				}else if(value == 7){
					$("#input-1446").val("tq");
					$("#input-1463").val("i.ind_4dig");
					$("#input-1450").val("ln_a~=. ");
				}else if(value == 8){
					$("#input-1446").val("tq");
					$("#input-1463").val("i.ind_4dig");
					$("#input-1450").val(" bank==0");
				}else if(value == 9){
					$("#input-1446").val("tq");
					$("#input-1463").val("i.ind_4dig");
					$("#input-1450").val("ind_4dig~=1165 & soe==0");
				}else if(value == 10){
					$("#input-1446").val("tq");
					$("#input-1463").val("i.ind_2dig");
					$("#input-1450").val("ch30==1");
				}else if(value == 11){
					$("#input-1446").val("tq");
					$("#input-1463").val("i.ind_4dig");
					$("#input-1450").val("ch30==0");
				}else if(value == 12){
					$("#input-1446").val("tq");
					$("#input-1463").val("i.ind_2dig");
					$("#input-1450").val("asize==1");
				}else if(value == 13){
					$("#input-1446").val("tq");
					$("#input-1463").val("i.ind_4dig");
					$("#input-1450").val("asset<2000 & asset>=80");
				}else if(value == 14){
					$("#input-1446").val("tq");
					$("#input-1463").val("i.ind_2dig");
					$("#input-1450").val(" asset<80 ");
