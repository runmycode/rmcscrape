<script>

	/**
	 * permet d inserer les donnes extraites d un fichier csv
	 * dans la variable options pour highchart 
	 * @param options : les options de highchart
	 * @param data decrivant le fichier csv != donnes retournees de la requete ajax 
	 * @returns options
	 */
	function getSeries(options, data) {
		// Split the lines
	    var lines = data.split('\n');
	    
	    // Iterate over the lines and add categories or series
	    $.each(lines, function(indexLines, line) {
	        var items = line.split(',');
	        if (indexLines == 0) {
	        	options.title.text = line;
	        } else {
	        	var series = {
	        		data: []
	        	};
	        	$.each(items, function(indexItems, value) {
	        		if (indexLines == 1) {
	        			series.color = '#4572A7';
	        		}
	        		if (indexItems == 0) {
	        			series.name = value;
	        		} else {
	        			series.data.push(parseFloat(value));
	        		}
	        	});
	        	options.series.push(series);
	        }
	    });
	    return options;
	}

	/**
	 * Affiche les graph dans l onglet demo data description
	 * @param data
	 * @param containerName le nom de l'id du conteneur dans la jsp
	 * @param color de fond du graphique
	 */
	function displayGraphDemoDataDescription(data, containerName) {
		var options = {	
			chart : {
				backgroundColor: 'none',
				borderColor : '#3a3a3a',
				renderTo : containerName,
				defaultSeriesType : 'line',
				borderWidth: 1,
				borderRadius : 5,
		        marginRight: 15,
		        marginLeft: 40,
		        marginBottm: 5,
		        style: {
		        	fontFamily: 'Arial, Helvetica, Verdana, sans-serif',
		        	fontSize: '10px'
		        }
			},
			title : {
				x : -20
			//center
			},
			subtitle : {
				text : null,
				x : -20
			},
			xAxis : {
				title : null,
				lineColor : '#3a3a3a',
				lineWidth: 1,
				labels: {
					style: {
			        	fontFamily: 'Arial, Helvetica, Verdana, sans-serif',
			        	fontSize: '10px',
			        	color : '#3a3a3a'
			        }
		        }
			},
			yAxis : {
				title : null,
				lineColor : '#3a3a3a',
				lineWidth: 1,
				labels: {
					style: {
			        	fontFamily: 'Arial, Helvetica, Verdana, sans-serif',
			        	fontSize: '10px',
			        	color : '#3a3a3a'
			        }
		        }
			},
			navigation: {
		        buttonOptions: {
		            enabled: false
		        }
		    },
			tooltip : {
				enabled : false
			},
			plotOptions : {
				series : {
					lineWidth : 1,
					animation : false,
					enableMouseTracking: false,
					shadow : false,
					marker : {
						enabled : false
					},
					states : {
						hover : {
							enabled : false
						}
					}
				}
			},
			credits : {
				enabled : false
			},
			legend : {
				layout : 'vertical',
				align : 'center',
				verticalAlign : 'top',
				x : 0,
				y : -10,
				borderWidth : 0,
				margin : 20,
				itemStyle: {
					cursor: 'pointer',
					color: '#3a3a3a',
					fontFamily: 'Arial, Helvetica, Verdana, sans-serif',
					fontSize: '10px'
				},
				itemHoverStyle: {
					color: '#3a3a3a'
				}
			},
			series : []
		};
		options = getSeries(options,data[0].results);
		var chart = new Highcharts.Chart(options);
	}

	/**
	 * recupere une chaine de caracter formate pour la generation d une courbe highchart
	 */
	function getDemoDataDescription(siteId, inputId, inputName) {
		var url = "ajaxDemoDataDescription.do";
		var type =  "POST";
		var dataType = "json";
		var params = {};
		params["siteId"] = siteId;
		params["inputId"] = inputId;
		params["inputName"] = inputName;
		var displaySuccess = function(data) {displayGraphDemoDataDescription(data, 'container-demo-data-description-' + inputId);};
		var displayError = function() {addMessage(errorMessage5, 1, "div-message");};
	    $.ajax({url : url , type : type,async: false , dataType : dataType , data : params , success: displaySuccess , error : displayError});
	}

