					$('#input-837').fieldcontrol('getDemoData');
				
					$('#input-831').fieldcontrol({rows_current: 0, columns_current: 0});		
					$('#input-831').fieldcontrol('getDemoData');
				
				
			    if(isDemoDataExist){
			    	$("#button-run-my-code").removeClass("button-disable");
					$("#button-run-my-code").addClass("button-enable");
			    }else{
			    	$("#button-run-my-code").removeClass("button-enable"); 
					$("#button-run-my-code").addClass("button-disable");
			    }
			    
			    isDemoData = true;
			}
		);
		
		$("#button-reset-data").unbind("click");
		$("#button-reset-data").click(
			function(){
				
				
					$('#input-815').fieldcontrol('reset');
				
					$('#input-824').fieldcontrol('reset');
				
					$('#input-823').fieldcontrol('reset');
				
					$('#input-825').fieldcontrol('reset');
				
					$('#input-837').fieldcontrol('reset');
				
					$('#input-831').fieldcontrol('reset');
				
				
				isDemoData = false;
				
				
			    
			    	$("#button-run-my-code").removeClass("button-disable"); 
		    		$("#button-run-my-code").addClass("button-enable");
			    
			    
				$("#cs-tab-bottom").tabs('disable', 3);
				$("#div-message").hide();
				
				if(isPreview){
					$("#div-preview-data").toggle('blind');
					isPreview = false;
					$("#button-preview-data").html("Preview data");
				}
			
			}
		);

		/*
		 *calcul de la hauteur de div-input
		 */
		var hmax = 0;
		
